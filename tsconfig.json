{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["esnext", "dom"],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "jsx": "preserve",
    "composite": true,
    "noImplicitAny": false,
    "noEmit": true,
  },
  "include": [
    "vite.config.ts", 
    "page/**/*.d.ts", 
    "page/**/*.vue", 
    "page/**/*.ts", 
    "main.ts", 
    "env.d.ts", 
    "routes.ts", 
    "note/ouline.ts",
    "page/component/note/prism/load-languages.js"
  ],
  //"include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"],
  "exclude": ["node_modules"]
}